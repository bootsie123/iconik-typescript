{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "7.6.0",
        "useDocker": true,
        "generators": {
            "iconik": {
                "generatorName": "typescript",
                "output": "src",
                "glob": "apis/iconik/*.spec.json",
                "additionalProperties": {
                    "npmName": "iconik-typescript",
                    "supportsES6": true,
                    "withInterfaces": true
                },
                "globalProperty": {
                    "apiDocs": true,
                    "modelDocs": true
                }
            }
        }
    }
}